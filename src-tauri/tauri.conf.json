{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "recfive",
  "version": "0.1.0",
  "identifier": "com.recfive.app",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist",
    "removeUnusedCommands": true
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "recfive",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}